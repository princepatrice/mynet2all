                    {% if user.type==6 %}
                    <li>
                        <a href="{{path("enseigne")}}">
                            <i class="material-icons">business</i>
                            <span>Enseignes</span>
                        </a>
                    </li>
                    {%endif%}
                </ul>
            </div>
